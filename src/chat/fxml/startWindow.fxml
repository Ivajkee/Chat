<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<HBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="chat.StartWindow">
    <Button contentDisplay="GRAPHIC_ONLY" onAction="#startServer">
        <graphic>
            <ImageView>
                <Image url="@../../files/img/ico/server.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button contentDisplay="GRAPHIC_ONLY" onAction="#startClient">
        <graphic>
            <ImageView>
                <Image url="@../../files/img/ico/client.png"/>
            </ImageView>
        </graphic>
    </Button>
</HBox>
